{
    "name": "open-ef/framework",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "php",
    "autoload": {
        "psr-4": {
            "OpenEf\\Framework\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OpenEfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.2",
        "illuminate/collections": "^8.0",
        "open-ef/container": "^1.0.x-dev",
        "symfony/console": "^5.4",
        "symfony/event-dispatcher": "^6.4",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": ">=7.0"
    },
    "minimum-stability": "dev",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
	    "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "open_ef": {
            "config": "OpenEf\\Framework\\ComponentProvider"
        }
    }
}
